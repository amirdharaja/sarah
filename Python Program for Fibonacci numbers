while True:
    def fib(n):
        a=0
        b=1
        if n<=0:
            print("Invalid input")
        elif(n==1):
            return 1
        else:
            for i in range(2,n):
                c=a+b
                a=b
                b=c
            return b
    n=int(input())
    print(fib(n))
